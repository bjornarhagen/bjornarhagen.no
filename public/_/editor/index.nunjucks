{% set title = "Editor" %}
{% set body_id = "editor-page" %}
{% set body_classes = "nav-small" %}
{% extends "default.nunjucks" %}
{% block content %}
  <main id="editor" data-editor-template="/_/editor/template.json"></main>
{% endblock %}
{% block scripts_after %}
  <link rel="stylesheet" href="/_/editor/editor.css">
  <script src="/_/editor/marked.js" defer></script>
  <script src="/_/editor/editor.js" defer></script>
{% endblock %}